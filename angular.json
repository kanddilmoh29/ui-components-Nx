{
  "version": 1,
  "projects": {
    "ui-components": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ui-components",
      "sourceRoot": "apps/ui-components/src",
      "prefix": "workspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ui-components",
            "index": "apps/ui-components/src/index.html",
            "main": "apps/ui-components/src/main.ts",
            "polyfills": "apps/ui-components/src/polyfills.ts",
            "tsConfig": "apps/ui-components/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/ui-components/src/favicon.ico",
              "apps/ui-components/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/ui-components/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ui-components/src/environments/environment.ts",
                  "with": "apps/ui-components/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ui-components:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ui-components:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ui-components:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ui-components/tsconfig.app.json",
              "apps/ui-components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ui-components/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ui-components/jest.config.js",
            "tsConfig": "apps/ui-components/tsconfig.spec.json",
            "setupFile": "apps/ui-components/src/test-setup.ts"
          }
        }
      }
    },
    "ui-components-e2e": {
      "root": "apps/ui-components-e2e",
      "sourceRoot": "apps/ui-components-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-components-e2e/cypress.json",
            "tsConfig": "apps/ui-components-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-components:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ui-components:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ui-components-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/ui-components-e2e/**"
            ]
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "workshop",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/material/tsconfig.lib.json",
              "libs/material/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/material/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "tsConfig": "libs/material/tsconfig.spec.json",
            "setupFile": "libs/material/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "core-data": {
      "projectType": "library",
      "root": "libs/core-data",
      "sourceRoot": "libs/core-data/src",
      "prefix": "workshop",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/core-data/tsconfig.lib.json",
              "libs/core-data/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/core-data/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core-data/jest.config.js",
            "tsConfig": "libs/core-data/tsconfig.spec.json",
            "setupFile": "libs/core-data/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "accordion": {
      "projectType": "library",
      "root": "libs/accordion",
      "sourceRoot": "libs/accordion/src",
      "prefix": "workshop",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/accordion/tsconfig.lib.json",
              "libs/accordion/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/accordion/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/accordion/jest.config.js",
            "tsConfig": "libs/accordion/tsconfig.spec.json",
            "setupFile": "libs/accordion/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "true"
        }
      }
    },
    "rxjs": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:class": {
          "skipTests": true
        },
        "@nrwl/angular:component": {
          "style": "scss",
          "skipTests": true
        },
        "@nrwl/angular:directive": {
          "skipTests": true
        },
        "@nrwl/angular:guard": {
          "skipTests": true
        },
        "@nrwl/angular:module": {
          "skipTests": true
        },
        "@nrwl/angular:pipe": {
          "skipTests": true
        },
        "@nrwl/angular:service": {
          "skipTests": true
        }
      },
      "root": "apps/rxjs",
      "sourceRoot": "apps/rxjs/src",
      "prefix": "workshop",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/rxjs",
            "index": "apps/rxjs/src/index.html",
            "main": "apps/rxjs/src/main.ts",
            "polyfills": "apps/rxjs/src/polyfills.ts",
            "tsConfig": "apps/rxjs/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/rxjs/src/favicon.ico",
              "apps/rxjs/src/assets"
            ],
            "styles": [
              "apps/rxjs/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/rxjs/src/environments/environment.ts",
                  "with": "apps/rxjs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "rxjs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "rxjs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rxjs:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/rxjs/tsconfig.app.json",
              "apps/rxjs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/rxjs/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/rxjs/jest.config.js",
            "tsConfig": "apps/rxjs/tsconfig.spec.json",
            "setupFile": "apps/rxjs/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/schematics:component": {
      "styleex": "scss"
    }
  },
  "defaultProject": "ui-components"
}